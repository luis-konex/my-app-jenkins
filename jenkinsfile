pipeline {
     agent any
     stages {
         stage('Build') {
             steps {
                 bat 'echo "Hello World"'
                 bat '''
                     echo "Multiline shell steps works too"
                     ls -lah
                 '''
             }
         }      
         stage('Upload to AWS') {
              steps {
                  withAWS(region:'us-east-1',credentials:'AWS_CREDENTIALS_ID') {
                  bat 'echo "Uploading content with AWS creds"'
                      s3Upload(pathStyleAccessEnabled: true, payloadSigningEnabled: true, file:'app.py', bucket:'test.rea')
                  }
              }
         }
     }
}